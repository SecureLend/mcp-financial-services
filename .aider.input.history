+typescriptimport axios, { AxiosInstance, AxiosRequestConfig, AxiosError } from 'axios';
+import {
+  SecureLendError,
+  AuthenticationError,
+  RateLimitError,
+  ValidationError,
+  NotFoundError,
+  NetworkError,
+  ServerError,
+} from './errors';
+
+interface APIClientConfig {
+  apiKey: string;
+  baseURL: string;
+  timeout: number;
+  maxRetries: number;
+  telemetry: boolean;
+}
+
+interface RequestOptions {
+  params?: Record<string, any>;
+  data?: Record<string, any>;
+  headers?: Record<string, string>;
+  idempotencyKey?: string;
+}
+
+export class APIClient {
+  private axios: AxiosInstance;
+  private config: APIClientConfig;
+  private debug: boolean = false;
+  
+  constructor(config: APIClientConfig) {
+    this.config = config;
+    
+    this.axios = axios.create({
+      baseURL: config.baseURL,
+      timeout: config.timeout,
+      headers: {
+        'Authorization': `Bearer ${config.apiKey}`,
+        'Content-Type': 'application/json',
+        'User-Agent': this.getUserAgent(),
+        ...(config.telemetry && {
+          'X-SecureLend-Client-User-Agent': JSON.stringify(this.getTelemetry()),
+        }),
+      },
+    });
+    
+    this.setupInterceptors();
+  }
+  
+  private setupInterceptors(): void {
+    // Request interceptor
+    this.axios.interceptors.request.use(
+      (config) => {
+        if (this.debug) {
+          console.log('[SecureLend SDK] Request:', {
+            method: config.method?.toUpperCase(),
+            url: config.url,
+            params: config.params,
+            data: config.data,
+          });
+        }
+        return config;
+      },
+      (error) => Promise.reject(error)
+    );
+    
+    // Response interceptor
+    this.axios.interceptors.response.use(
+      (response) => {
+        if (this.debug) {
+          console.log('[SecureLend SDK] Response:', {
+            status: response.status,
+            data: response.data,
+          });
+        }
+        return response;
+      },
+      (error) => this.handleError(error)
+    );
+  }
+  
+  /**
+   * Make an API request with automatic retries
+   */
+  async request<T>(
+    method: string,
+    path: string,
+    options: RequestOptions = {}
+  ): Promise<T> {
+    const config: AxiosRequestConfig = {
+      method,
+      url: path,
+      params: options.params,
+      data: options.data,
+      headers: {
+        ...options.headers,
+        ...(options.idempotencyKey && {
+          'Idempotency-Key': options.idempotencyKey,
+        }),
+      },
+    };
+    
+    let lastError: Error;
+    
+    for (let attempt = 0; attempt <= this.config.maxRetries; attempt++) {
+      try {
+        const response = await this.axios.request<T>(config);
+        return response.data;
+      } catch (error) {
+        lastError = error as Error;
+        
+        // Don't retry on client errors (4xx) except 429
+        if (axios.isAxiosError(error)) {
+          const status = error.response?.status;
+          
+          if (status && status >= 400 && status < 500 && status !== 429) {
+            throw this.handleError(error);
+          }
+          
+          // Don't retry on last attempt
+          if (attempt === this.config.maxRetries) {
+            throw this.handleError(error);
+          }
+          
+          // Exponential backoff with jitter
+          const baseDelay = Math.min(1000 * Math.pow(2, attempt), 10000);
+          const jitter = Math.random() * 1000;
+          const delay = baseDelay + jitter;
+          
+          if (this.debug) {
+            console.log(`[SecureLend SDK] Retry attempt ${attempt + 1} after ${delay}ms`);
+          }
+          
+          await this.sleep(delay);
+        } else {
+          throw error;
+        }
+      }
+    }
+    
+    throw lastError!;
+  }
+  
+  setApiKey(apiKey: string): void {
+    this.config.apiKey = apiKey;
+    this.axios.defaults.headers.common['Authorization'] = `Bearer ${apiKey}`;
+  }
+  
+  enableDebug(): void {
+    this.debug = true;
+  }
+  
+  disableDebug(): void {
+    this.debug = false;
+  }
+  
+  private handleError(error: AxiosError): never {
+    if (!error.response) {
+      throw new NetworkError(
+        'Network error - please check your internet connection'
+      );
+    }
+    
+    const { status, data } = error.response;
+    const errorData = data as any;
+    
+    switch (status) {
+      case 401:
+        throw new AuthenticationError(
+          errorData?.message || 'Invalid API key'
+        );
+      
+      case 429:
+        throw new RateLimitError(
+          errorData?.message || 'Rate limit exceeded',
+          errorData?.retryAfter
+        );
+      
+      case 400:
+        throw new ValidationError(
+          errorData?.message || 'Invalid request parameters',
+          errorData?.errors
+        );
+      
+      case 404:
+        throw new NotFoundError(
+          errorData?.message || 'Resource not found'
+        );
+      
+      case 500:
+      case 502:
+      case 503:
+      case 504:
+        throw new ServerError(
+          errorData?.message || 'Server error - please try again later'
+        );
+      
+      default:
+        throw new SecureLendError(
+          errorData?.message || 'An unexpected error occurred',
+          'unknown_error',
+          { status, data: errorData }
+        );
+    }
+  }
+  
+  private getUserAgent(): string {
+    // In real implementation, get version from package.json
+    const version = '1.0.0';
+    const nodeVersion = typeof process !== 'undefined' ? process.version : 'unknown';
+    const platform = typeof process !== 'undefined' ? process.platform : 'unknown';
+    
+    return `SecureLend-SDK/${version} (${platform}; Node/${nodeVersion})`;
+  }
+  
+  private getTelemetry() {
+    return {
+      name: '@securelend/sdk',
+      version: '1.0.0',
+      language: 'node',
+      language_version: typeof process !== 'undefined' ? process.version : 'unknown',
+      platform: typeof process !== 'undefined' ? process.platform : 'unknown',
+    };
+  }
+  
+  private sleep(ms: number): Promise<void> {
+    return new Promise((resolve) => setTimeout(resolve, ms));
+  }
+}
